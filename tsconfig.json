{
  "compilerOptions": {
    "noEmit": false,
    "module": "es6",
    "target": "es5",
    "outDir": "./assets/dist/",
    "rootDir": "./assets/src/",
    "moduleResolution": "node",
    //"strict": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "skipLibCheck": true
  },
  "exclude": [
    "**/node_modules",
    "**/assets/dist"
  ]
}
